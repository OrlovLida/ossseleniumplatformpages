<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="My Changes, My Group Changes and Closed Changes Tests" thread-count="2" parallel="tests" verbose="1">
    <test name="MyGroupChanges">
        <parameter name="changeRequester" value="sd_seleniumtest"/>
