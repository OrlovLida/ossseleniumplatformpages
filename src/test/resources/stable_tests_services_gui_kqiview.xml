<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ServicesGuiSQM-CRUD" thread-count="1" parallel="tests" verbose="1">
    <parameter name="indicatorNodesToExpand" value="DFE Tests,DFE Product Tests,Selenium Tests"/>
    <parameter name="indicatorNodesToSelect" value="CPU_NICE_USAGE,CPU_TOTAL_USAGE"/>
    <parameter name="dimensionNodesToExpand" value="t:SMOKE#D_HOST"/>
    <parameter name="dimensionNodesToSelect" value="192.168.128.172,192.168.128.173"/>
    <test name="BasicKpiViewTest">
        <classes>
            <class name="com.oss.bigdata.kpiview.KpiViewTest">
                <methods>
                    <exclude name="verifyNumberOfRangesAndLine" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="TopNKpiViewTest">
        <parameter name="indicatorNodesToSelect" value="t:SMOKE#Attempts"/>
        <parameter name="dimensionNodesToExpand" value="d1"/>
        <parameter name="dimensionNodesToSelect" value="D1_01"/>
        <classes>
            <class name="com.oss.bigdata.kpiview.KpiViewTest">
                <methods>
                    <include name="verifyNumberOfRangesAndLine"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="SharePanelTest">
        <parameter name="indicatorNodesToExpand" value="DFE Tests,DFE Product Tests,Selenium Tests"></parameter>
        <parameter name="indicatorNodesToSelect" value="SUCCESS_LONG"></parameter>
        <parameter name="dimensionNodesToExpand" value="t:SMOKE#DimHierSelenium"></parameter>
        <parameter name="dimensionNodesToSelect" value="D3_01"></parameter>
        <parameter name="filterName" value="DFE Tests"></parameter>
        <classes>
            <class name="com.oss.dpe.IndicatorsViewTest">
                <methods>
                    <include name="SharePanelTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
